    cb n = n * n * n
    cbrt 0 = [0]
    cbrt 1 = [1]
    cbrt 8 = [2]
    cbrt 27 = [3]
    cbrt 64 = [4]
    cbrt 125 = [5]
    cbrt 216 = [6]
    cbrt 343 = [7]
    cbrt 512 = [8]
    cbrt 729 = [9]
    cbrt 1000 = [10]
    cbrt 1331 = [11]
    cbrt 1728 = [12]
    cbrt n = []
    s = {[a,b,c];a<-[-9..-1];b<-[a..9];c<-cbrt (100 - cb a - cb b)}
